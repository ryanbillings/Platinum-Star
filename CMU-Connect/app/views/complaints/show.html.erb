<% title "Complaint" %>

<p>
  <strong>Reported User:</strong>
  <%= @complaint.to_user %>
</p>
<p>
  <strong>Submitted By:</strong>
  <%= @complaint.from_user %>
</p>
<p>
  <strong>Description:</strong>
  <%= @complaint.description %>
